{
    "name": "@cel_cash/client",
    "version": "1.0.6",
    "description": "A client sdk for CelCash",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "type-check": "tsc --noEmit",
        "build": "tsup",
        "release": "pnpm release-it"
    },
    "files": ["dist"],
    "keywords": ["celcash", "client", "sdk", "api", "typescript"],
    "author": "Walaff Fernandes",
    "license": "ISC",
    "homepage": "https://github.com/Pyxlab/celcash/tree/main/packages/client#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Pyxlab/celcash.git",
        "directory": "packages/client"
    },
    "bugs": {
        "url": "https://github.com/Pyxlab/celcash/issues"
    },
    "devDependencies": {
        "@cel_cash/core": "1.0.6",
        "@swc/core": "^1.5.7",
        "@types/node": "^20.12.12",
        "tsup": "^8.0.2",
        "typescript": "^5.4.5"
    },
    "peerDependencies": {
        "@cel_cash/core": "^1.0.0"
    },
    "dependencies": {
        "@ts-rest/core": "^3.45.2"
    },
    "publishConfig": {
        "access": "public"
    },
    "release-it": {
        "git": false,
        "plugins": {
            "@release-it/bumper": {
                "out": {
                    "file": "package.json",
                    "path": ["devDependencies.@cel_cash/core"]
                }
            }
        }
    }
}
