{
    "name": "@cel_cash/core",
    "version": "2.2.7",
    "description": "Core ts-rest module for CelCash",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "lint": "pnpm biome check src --apply",
        "type-check": "tsc --noEmit",
        "build": "tsup",
        "release": "pnpm release-it"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        },
        "./types": {
            "import": "./dist/types.js",
            "require": "./dist/types.cjs",
            "types": "./dist/types.d.ts"
        },
        "./schemas": {
            "import": "./dist/schemas/index.js",
            "require": "./dist/schemas/index.cjs",
            "types": "./dist/schemas/index.d.ts"
        },
        "./contracts": {
            "import": "./dist/contract/index.js",
            "require": "./dist/contract/index.cjs",
            "types": "./dist/contract/index.d.ts"
        },
        "./utils": {
            "import": "./dist/utils/index.js",
            "require": "./dist/utils/index.cjs",
            "types": "./dist/utils/index.d.ts"
        }
    },
    "files": [
        "dist",
        "schemas.d.ts",
        "contracts.d.ts",
        "types.d.ts"
    ],
    "keywords": [
        "celcash",
        "sdk",
        "api",
        "typescript"
    ],
    "author": "Walaff Fernandes",
    "license": "ISC",
    "homepage": "https://github.com/Pyxlab/celcash/tree/main/packages/core#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Pyxlab/celcash.git",
        "directory": "packages/core"
    },
    "bugs": {
        "url": "https://github.com/Pyxlab/celcash/issues"
    },
    "peerDependencies": {
        "@ts-rest/core": "^3.45.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@swc/core": "^1.5.7",
        "@types/node": "^20.14.2",
        "tsup": "^8.0.2",
        "typescript": "^5.4.5",
        "vitest": "^1.6.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "release-it": {
        "git": false,
        "hooks": {
            "before:init": [
                "pnpm lint",
                "pnpm type-check"
            ],
            "after:bump": "npm run build",
            "after:git:release": "echo After git push, before github release"
        }
    },
    "dependencies": {
        "ts-pattern": "^5.1.2"
    }
}
